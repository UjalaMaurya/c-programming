#include <iostream>
using namespace std;

int main() {    
   cout << "\t \t\t\t\t\t Voting Eligibility test";
  
   int  age;
   cout  <<  "\nEnter your age : ";
   cin>>age;
    
    if ( age  >= 18 ) {
    	cout  <<  "\n congratulations...  \nYou are eligible to vote";
    	}  else {
   	cout << "\n  sorry...   \nYou are not eligible to vote";
   	}

    return 0;
}
